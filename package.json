{
  "name": "ng-class-list-extractor",
  "version": "0.2.0",
  "description": "Determine what CSS classes an Angular Template or Component uses",
  "keywords": [
    "angular",
    "angular-cli",
    "ng",
    "ng-tailwind",
    "css",
    "tailwind",
    "tailwindcss",
    "purgecss"
  ],
  "main": "lib/api.js",
  "repository": "https://github.com/vikaspotluri123/ng-class-list-extractor",
  "author": "Vikas Potluri <vikaspotluri123.github@gmail.com>",
  "license": "MIT",
  "private": false,
  "devDependencies": {
    "xo": "^0.34.2"
  },
  "peerDependencies": {
    "@angular/compiler": ">=10.0.0 < 13.0.0",
    "typescript": ">3.9"
  },
  "xo": {
    "rules": {
      "func-names": [
        2,
        "as-needed"
      ]
    },
    "overrides": [
      {
        "files": [
          "test/**/*"
        ],
        "env": [
          "mocha"
        ],
        "rules": {
          "prefer-arrow-callback": "off",
          "func-names": "off"
        }
      }
    ]
  }
}
